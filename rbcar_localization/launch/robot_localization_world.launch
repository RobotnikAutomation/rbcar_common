<?xml version="1.0"?>
<launch>  
	
	<arg name="prefix" default=""/>

    <node pkg="robot_localization" type="ekf_localization_node" name="filter_world" clear_params="true" output="screen">
        <rosparam command="load" file="$(find rbcar_localization)/config/filter_world.yaml" />
        <remap from="odometry/filtered" to="odometry/filtered_world"/>
    </node>
  
    <node pkg="robot_localization" type="navsat_transform_node" name="navsat_transform" clear_params="true" output="screen" >
        <rosparam command="load" file="$(find rbcar_localization)/config/navsat.yaml" />
        <remap from="odometry/filtered" to="odometry/filtered_world"/>
    </node>

</launch>
